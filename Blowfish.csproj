<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="assets\info.svg" />
    <None Remove="assets\lock.svg" />
    <None Remove="assets\unlock.svg" />
    <None Remove="folder.svg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SharpVectors.Reloaded" Version="1.6.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="assets\folder.svg" />
    <Resource Include="assets\info.svg" />
    <Resource Include="assets\lock.svg" />
    <Resource Include="assets\unlock.svg" />
  </ItemGroup>

</Project>